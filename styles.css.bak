body {
	background-color: #333 !important;
	color: #fff !important;
}

.player {
	background-color: #333 !important;
	border-bottom: 1px solid #888;
	padding-bottom: .25em;
}
.player:last-child {
	border: none;
}

.playerName.playerVoted {
	background-color: #333 !important;
}

.playerName .icon-ok {
	vertical-align: baseline;
	background-color: #bbb;
}

.voteHidden {
	background-color: #111 !important;
}

{
  "manifest_version": 2,

  "name": "PointingPoker Darkmode Plugin",
  "description": "This extension gives a nice darkmode to pointingpoker.com",
  "version": "1.0",

  "content_scripts": [{
	"css": ["styles.css"],
	"matches": ["https://pointingpoker.com/*"]
  }],
  "permissions": [
   "activeTab"
   ]
}